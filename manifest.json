{
    "name": "Tab Stream",
    "description": "Records the current tab and stream for server",
    "version": "1",
    "manifest_version": 3,
    "minimum_chrome_version": "116",
    "action": {
      "default_icon": "icons/not-recording.png"
    },
    "background": {
      "service_worker": "service-worker.js"
    },
    "permissions": ["tabCapture", "offscreen"]
  }